{
  "name": "front-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-arm": "ng serve arm",
    "start-arm-mock": "ng serve arm --configuration=mock",
    "build-arm": "ng build arm --outputPath=../Happylend.ARM/front-debug/",
    "build-arm-mock": "ng build arm --configuration=mock --outputPath=../Happylend.ARM/front-debug/",
    "build-arm-prod": "ng build arm --prod --outputPath=../Happylend.ARM/front/ --stats-json && gulp inject",
    "build-client-form": "npx gulp css-client-form && ng build client-form --outputPath=../Happylend.NewClient/front-debug/",
    "start-client-form": "npx gulp css-client-form && ng serve client-form",
    "watch-styles-client-form": "npx gulp watch-css-client-form",
    "build-client-form-mock": "ng build client-form --configuration=mock --outputPath=../Happylend.NewClient/front-debug/",
    "build-client-form-prod": "npx gulp css-client-form && ng build client-form --prod --outputPath=../Happylend.NewClient/front/ && gulp inject-client"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "angular2-text-mask": "^9.0.0",
    "moment": "^2.24.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-bom": "^3.0.0",
    "gulp-csso": "4.0.1",
    "gulp-inject": "^5.0.5",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "stream-series": "^0.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
